def solution(prices):
    answer = []
    term=0
    for i in range(len(prices)):
        for j in range(i+1,len(prices)):
            if(prices[i]<=prices[j]):
                term+=1
            else:
                #if len(prices)-1 != i:
                term+=1   
                break #
        answer.append(term)
        term=0
    return answer
